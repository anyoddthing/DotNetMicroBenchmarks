#root-main {
  width: 100%;
  height: 100%;
}

.bench {
  padding: var(--appui-spacing-275);
  align-items: flex-start;
}

/*.appui--editor-dark .bench,*/
/*.appui--editor-light .bench {*/
/*  background-color: var(--appui-backgrounds-200);*/
/*}*/

.appui--rtl .bench {
  align-items: flex-end;
}

.bench-header {
  height: var(--appui-sizing-1200);
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  align-self: stretch;
}

.appui--rtl .bench-header {
  flex-direction: row-reverse;
}

.bench-title {
  color: var(--appui-foregrounds-100);
  -unity-font-definition: url('/Packages/com.unity.dt.app-ui/PackageResources/Fonts/NotoSans-Black.ttf');
  font-size: var(--appui-font-sizes-1000);
  margin: var(--appui-spacing-400) 0 0 0;
  align-self: stretch;
}

.bench-subtitle {
  color: var(--appui-foregrounds-100);
  font-size: var(--appui-font-sizes-100);
  margin: var(--appui-spacing-400) 0;
  padding: var(--appui-alias-control-gap-md);
  border-radius: var(--appui-alias-control-border-radius);
  align-self: flex-start;
  background-color: var(--appui-base-50);
}

.appui--rtl .bench-subtitle {
  align-self: flex-end;
}

.bench-section {
  /*border-width: 1px;*/
  /*border-color: #7B61FF;*/
  border-radius: var(--appui-alias-control-border-radius);
  flex-grow: 0;
  flex-shrink: 0;
  padding: var(--appui-spacing-275);
  flex-wrap: nowrap;
  width: 450px;
  max-width: 97%;
  background-color: var(--appui-backgrounds-200);
}

.align-start {
  align-items: flex-start;
}

.bench-section-fullwidth {
  align-self: stretch;
  align-items: center;
}

.bench-section > .appui-button {
  margin: var(--appui-alias-control-gap-md) 0;
}

.bench-row {
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  flex-wrap: nowrap;
  flex-grow: 0;
  flex-shrink: 0;
  max-width: 100%;
  padding: var(--appui-alias-control-gap-md) 0;
}

.bench-item {
  display: flex;
  flex-direction: column;
  align-items: center;
  padding: 0 var(--appui-alias-control-gap-md);
  flex-shrink: 0;
  flex-grow: 0;
}

.bench-column {
  display: flex;
  flex-direction: column;
}

.bench-context-switcher-panel {
  position: absolute;
  right: var(--appui-sizing-400);
  max-height: 90%;
  top: 50%;
  translate: 0 -50%;
  border-radius: var(--appui-alias-control-border-radius);
  background-color: var(--appui-backgrounds-200);
}

.bench-context-switcher-panel__content {
  padding: var(--appui-alias-control-gap-xl) var(--appui-sizing-500) var(--appui-alias-control-gap-xl) var(--appui-alias-control-gap-xl);
}

.bench-context-switcher-panel__title {
  margin: var(--appui-alias-control-gap-lg) 0 var(--appui-alias-control-gap-md) 0;
}

.bench-context-switcher-panel__switcher {
  margin-bottom: var(--appui-alias-control-gap-lg);
}

.appui--rtl .bench-context-switcher-panel {
  left: 18px;
  right: auto;
}

.appui-linear-progress {
  margin: var(--appui-alias-control-gap-md) 0;
}

.bench-cp-with-text {
  width: var(--appui-sizing-1000);
  height: var(--appui-sizing-1000);
}

.appui--large .bench-context-switcher-panel {
  right: 24px;
}

.appui-swipeview {
  background-color: var(--appui-backgrounds-100);
}

#swipeview-distance {
  background-color: rgba(0,0,0,0);
}

.appui-swipeview-item > .appui-avatar {
  cursor: var(--appui-alias-control-cursor-pointer);
}

.appui-swipeview-item.is-selected > .appui-avatar > .appui-avatar__container {
  border-color: #2369ff;
  border-width: 2px;
}

.appui-stackview {
  height: 300px;
  margin-bottom: var(--appui-alias-control-gap-lg);
  background-color: var(--appui-backgrounds-200);
}

.appui-stackview-item > .appui-text {
  width: 100px;
  height: 100px;
  -unity-text-align: middle-center;
  background-color: var(--appui-backgrounds-300);
  border-width: 2px;
  border-color: var(--appui-accent-100);
  border-radius: 8px;
}

.appui-pageview .appui-text {
  width: 150px;
  height: 150px;
  -unity-text-align: middle-center;
  background-color: var(--appui-backgrounds-300);
  border-width: 2px;
  border-color: var(--appui-accent-100);
  border-radius: 8px;
}

.appui-splitview {
  background-color: var(--appui-backgrounds-300);
}

.bench-elevation-card {
  width: 36%;
  height: 40px;
  margin: 24px;
  border-radius: 2px;
  background-color: white;
  justify-content: center;
  align-items: center;
}

.bench-elevation-card > .appui-text {
  color: rgb(143, 143, 143);
}

.badge-container-bench {
  width: 40px;
  height: 40px;
  align-items: stretch;
  border-radius: var(--appui-alias-control-border-radius);
  background-color: var(--appui-backgrounds-100);
}

.badge-container-circular-bench {
  border-radius: 40px;
}

.appui-grid-view__item .appui-text {
  width: 100%;
  height: 100%;
  -unity-text-align: middle-center;
  background-color: var(--appui-backgrounds-300);
  border-width: 2px;
  border-color: rgba(0,0,0,0);
  border-radius: var(--appui-alias-control-border-radius);
}

.appui-grid-view__item.is-selected .appui-text {
  border-color: var(--appui-accent-100);
}